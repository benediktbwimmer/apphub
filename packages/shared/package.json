{
  "name": "@apphub/shared",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./duckdb": {
      "types": "./dist/duckdb.d.ts",
      "require": "./dist/duckdb.js",
      "default": "./dist/duckdb.js"
    },
    "./exampleJobBundles": {
      "types": "./dist/exampleJobBundles.d.ts",
      "require": "./dist/exampleJobBundles.js",
      "default": "./dist/exampleJobBundles.js"
    },
    "./postgres": {
      "types": "./dist/postgres.d.ts",
      "require": "./dist/postgres.js",
      "default": "./dist/postgres.js"
    }
  },
  "scripts": {
    "build": "tsc -b"
  },
  "dependencies": {
    "@apphub/examples-registry": "0.1.0",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/pg": "^8.15.5"
  }
}
