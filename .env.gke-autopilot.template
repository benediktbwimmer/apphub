# Copy this file to `.env.gke-autopilot`, fill in the values, then `source` it
# before running the Autopilot tooling (bootstrap/deploy scripts).

# Required identifiers
APPHUB_GKE_PROJECT=your-gcp-project-id
APPHUB_GKE_TIMESTORE_BUCKET=your-gcs-bucket-name

# Artifact Registry configuration
APPHUB_GKE_REGION=europe-west1
APPHUB_GKE_REPO=apphub
APPHUB_GKE_IMAGE_TAG=autopilot-YYYYMMDD
APPHUB_GKE_NAMESPACE=apphub-system

# ClickHouse cold storage credentials (HMAC key)
APPHUB_GKE_CLICKHOUSE_ACCESS_KEY=replace-with-clickhouse-hmac-access-key
APPHUB_GKE_CLICKHOUSE_SECRET_KEY=replace-with-clickhouse-hmac-secret-key
APPHUB_GKE_CLICKHOUSE_PASSWORD=replace-with-clickhouse-user-password

# Optional: baked into the frontend image during deploy (defaults to http://localhost:4000)
APPHUB_FRONTEND_API_BASE_URL=https://demo.osiris-apphub.com/api
