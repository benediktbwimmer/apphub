{
  "name": "catalog",
  "version": "0.1.0",
  "description": "Metadata and search API for containerized web applications",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "lint": "tsc --noEmit",
    "build": "tsc",
    "start": "node dist/server.js",
    "ingest": "tsx src/ingestionWorker.ts",
    "builds": "tsx src/buildWorker.ts",
    "launches": "tsx src/launchWorker.ts",
    "workflows": "tsx src/workflowWorker.ts",
    "test:e2e": "APPHUB_EVENTS_MODE=inline tsx tests/ingestion.e2e.ts && APPHUB_EVENTS_MODE=inline tsx tests/jobs.e2e.ts && APPHUB_EVENTS_MODE=inline tsx tests/jobRegistry.e2e.ts && APPHUB_EVENTS_MODE=inline tsx tests/workflows.e2e.ts"
  },
  "keywords": [
    "webapps",
    "catalog",
    "search"
  ],
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.614.0",
    "@aws-sdk/s3-request-presigner": "^3.614.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/websocket": "^8.3.1",
    "bullmq": "^5.9.2",
    "fastify": "^4.27.2",
    "ioredis": "^5.4.1",
    "pg": "^8.12.0",
    "simple-git": "^3.26.0",
    "yaml": "^2.5.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/pg": "^8.11.4",
    "@types/ws": "^8.18.1",
    "embedded-postgres": "^17.6.0-beta.15",
    "ioredis-mock": "^8.9.0",
    "redis-server": "^1.2.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  }
}
