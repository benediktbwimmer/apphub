{
  "name": "@apphub/timestore",
  "version": "0.1.0",
  "description": "DuckDB-backed time series storage service",
  "main": "dist/server.js",
  "type": "commonjs",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "build": "npm run build:ts",
    "build:ts": "tsc -b",
    "lint": "tsc -b --pretty false",
    "lifecycle": "tsx src/workers/lifecycleWorker.ts",
    "test": "tsx --test tests/**/*.test.ts"
  },
  "dependencies": {
    "@apphub/shared": "0.1.0",
    "fastify": "^5.6.1",
    "pg": "^8.16.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "@types/pg": "^8.15.5",
    "embedded-postgres": "^17.6.0-beta.15",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
