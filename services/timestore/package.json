{
  "name": "@apphub/timestore",
  "version": "0.1.0",
  "description": "DuckDB-backed time series storage service",
  "main": "dist/server.js",
  "type": "commonjs",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "build": "npm run build:ts && npm run build:openapi",
    "build:ts": "tsc -b",
    "build:openapi": "tsx scripts/generateOpenApi.ts",
    "lint": "tsc -b --pretty false",
    "lifecycle": "tsx src/workers/lifecycleWorker.ts",
    "ingest": "tsx src/workers/ingestionWorker.ts",
    "partition-build": "tsx src/workers/partitionBuildWorker.ts",
    "test": "tsx tests/runE2E.ts",
    "prime-cache": "tsx src/tools/primeManifestCache.ts",
    "schema-migrate": "tsx src/tools/schemaMigration.ts"
  },
  "dependencies": {
    "@apphub/shared": "0.1.1",
    "@fastify/swagger": "^9.5.2",
    "@fastify/swagger-ui": "^5.2.3",
    "@aws-sdk/client-s3": "^3.896.0",
    "@aws-sdk/lib-storage": "^3.896.0",
    "@azure/storage-blob": "^12.28.0",
    "@opentelemetry/api": "^1.9.0",
    "bullmq": "^5.58.8",
    "@google-cloud/storage": "^7.17.1",
    "fastify": "^5.6.1",
    "@apphub/event-bus": "0.1.0",
    "fastify-plugin": "^4.5.1",
    "ioredis": "^5.8.0",
    "kafkajs": "^2.2.4",
    "pg": "^8.16.3",
    "js-yaml": "^4.1.0",
    "prom-client": "^15.1.3",
    "openapi-types": "^12.1.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "@types/pg": "^8.15.5",
    "@types/js-yaml": "^4.0.9",
    "embedded-postgres": "^17.6.0-beta.15",
    "s3rver": "^3.7.1",
    "tsx": "^4.20.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
